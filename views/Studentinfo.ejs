<h1>Student Information</h1>
<br>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Phone Number</th>
        <th>parent's gmail</th>
        <th>student Email</th>
      </tr>
    </thead>
    <tbody>
      <% students.forEach(student => { %>
        <tr>
          <td><%= student.name %></td>
          <td><%= student.studentNO %></td>
          <td><%= student.parentsno %></td>
          <td><%= student.email %></td>
          <!-- Add more fields as needed -->
        </tr>
      <% }); %>
